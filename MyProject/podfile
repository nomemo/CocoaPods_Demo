workspace 'MyProject'
xcodeproj 'MyProject.xcodeproj'
xcodeproj '../MyMacProject/MyMacProject.xcodeproj'


target 'MyProject' do
    platform :ios, '7.0'
	pod "AFNetworking" 
    pod "MyLocalLib", :path => "../MyLocalLib"
	xcodeproj 'MyProject.xcodeproj'

end


target 'MyMacProject' do
    platform :osx, '10.9'
	pod "AFNetworking" 
    pod "MyLocalLib", :path => "../MyLocalLib"
	xcodeproj 'MyMacProject.xcodeproj'
end





